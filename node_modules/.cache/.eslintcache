         {/* C */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/c.png\" alt=\"\" />\n        <div className=\"image-overlay\">C</div>\n        </div>\n        {/* C++ */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/cpp.png\" alt=\"\" />\n        <div className=\"image-overlay\">C++</div>\n        </div>\n        {/* Python */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/python.png\" alt=\"\" />\n        <div className=\"image-overlay\">Python</div>\n        </div>\n        {/* Java */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/java.png\" alt=\"\" />\n        <div className=\"image-overlay\">Java</div>\n        </div>\n       \n      </div>\n      <hr/>\n      <div className=\"Center\">\n        <p>Web/App based tech stacks</p>\n      </div>\n      <div className=\"skill-image-container\">\n        {/* HTML */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/html.png\" alt=\"\" />\n        <div className=\"image-overlay\">HTML</div>\n        </div>\n        {/* CSS */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/css.png\" alt=\"\" />\n        <div className=\"image-overlay\">CSS</div>\n        </div>\n        {/* javascript */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/javascript.png\" alt=\"\" />\n        <div className=\"image-overlay\">Javascript</div>\n        </div>\n        {/* Firebase */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/firebase.png\" alt=\"\" />\n        <div className=\"image-overlay\">Firebase</div>\n        </div>\n        <img className=\"skill-image\" src=\"/Images/react.png\" alt=\"\" /> \n         \n        <img className=\"skill-image\" src=\"/Images/mysql.png\" alt=\"\" />  \n        \n        \n      </div>\n      <hr/>\n      <div className=\"Center\">\n        <p>Robotics</p>\n      </div>\n      <div className=\"skill-image-container\">\n        {/* ROS-1 */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/ROS-1.png\" alt=\"\" style={{ width:250}}/>\n        <div className=\"image-overlay\">Robot Operating Systems</div>\n        </div> \n        {/* ROS-2 */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/ROS2.jpg\" alt=\"\" style={{ width:250}}/>\n        <div className=\"image-overlay\">Robot Operating Systems 2</div>\n        </div> \n        {/* Gazebo */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/gazebo.png\" alt=\"\"/>\n        <div className=\"image-overlay\">Gazebo Simulator</div>\n        </div> \n        {/* Coppelia Simulator */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/coppelia.png\" alt=\"\"/>\n        <div className=\"image-overlay\">Coppelia Simulator</div>\n        </div>\n      </div>\n      \n      <br/>\n      <div className=\"Center\">\n        <p>Open Source Frameworks</p>\n      </div>\n      <div className=\"skill-image-container\">\n        {/* OpenCv */}\n      <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/opencv.png\" alt=\"\"/>\n        <div className=\"image-overlay\">OpenCV</div>\n        </div>\n        {/* OpenAI */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/openai-logomark.png\" alt=\"\"/>\n        <div className=\"image-overlay\">OpenAI - Gymnasium</div>\n        </div>\n        {/* Pytorch */}\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/pytorch.png\" alt=\"\" style={{ width:250}}/>\n        <div className=\"image-overlay\">Pytorch</div>\n        </div>\n      </div>\n      <hr/>\n        \n      <div className=\"Center\">\n        <p>These are the primary Microprocessors / Hardware boards I've worked with</p>\n      </div>\n      <div className=\"skill-image-container\">\n      <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/arduino.png\" alt=\"\"/>\n        <div className=\"image-overlay\">Arduino</div>\n        </div>\n\n      <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/raspberry.png\" alt=\"\"/>\n        <div className=\"image-overlay\">Raspberry Pi</div>\n        </div>\n\n        <div className=\"image-with-overlay\">\n        <img className=\"skill-image\" src=\"/Images/nvidia.png\" alt=\"\"/>\n        <div className=\"image-overlay\">Nvidia Xavier/Jetson</div>\n        </div>\n        \n        <br/>\n        <br/>\n\n      \n      </div>\n    </div>\n    </div> \n\n  );\n}\n\n  ","C:\\Users\\aksha\\OneDrive\\Desktop\\portfolio\\src\\Component\\Contact.js",["105","106","107","108","109","110","111","112"],[],"import React, {Component, Fragment} from 'react';\r\n// import { Link, NavLink } from 'react-router-dom';\r\nimport { BrowserRouter as Router, Route, Routes, Link, Outlet } from 'react-router-dom';\r\nimport Navbar from './Navbar';\r\n\r\n// import About from \"./Component/About\";\r\n// import Contact from \"./Component/Contact\";\r\n// import Projects from \"./Component/Projects\";\r\n\r\n\r\nexport default function Contact() {\r\n    return (\r\n        <div className=\"Center\">\r\n        <div>\r\n        <h1>Contact</h1>\r\n        <p></p>\r\n        </div>\r\n        </div>\r\n        \r\n      );\r\n}\r\n\r\n\r\n\r\n","C:\\Users\\aksha\\OneDrive\\Desktop\\portfolio\\src\\Component\\About.js",["113","114","115"],[],"import React from 'react';\r\nimport { Link, NavLink } from 'react-router-dom';\r\nimport Navbar from './Navbar';\r\n\r\nfunction About() {\r\n    return (\r\n      <div className=\"Center\">\r\n      <div className=\"header-container\">\r\n      <div className=\"top-bar\">\r\n      </div>\r\n      <br/>\r\n    \r\n    <h1>Experience</h1>\r\n    <p>\r\n    <p> I've researched and worked with robots a lot, I managed to gain some useful skills while trying to work<br/>\r\n       on autonomous systems throughout my undergraduate degree. My primary area of work has been towards <br/>Control Systems and heuristic path-planning\r\n       of Unmanned Aerial Vehicles and Unmanned Ground Vehicles.<br/></p>\r\n    <div>\r\n      <ul>\r\n        <li>Software Engineering Intern, Analog Devices, India <i>Jan 2023 - May 2023</i></li>\r\n        <li>Undergraduate Research Intern (MITACS), MARS Lab, Ontario Tech University <i>May 2022 - Jul 2022</i></li>\r\n        <li>Head of Automation, Project MANAS <i>April 2021 - Jul 2022</i></li>\r\n        <li>Embedded Software Intern, AEREO, India <i>Oct 2022 - Dec 2022</i></li>\r\n\r\n        <li>Team Member, MBZIRC <i>Oct 2022 - Dec 2022</i></li>\r\n      </ul>\r\n    </div>\r\n    </p>\r\n    \r\n    <h1>My Takeaways</h1>\r\n       <h2>My time at Analog Devices</h2>\r\n      <p>Before arriving for my Master's, I was a Software Engineering Intern at Analog Devices, India or ADI.<br/> \r\n       ADI was working on a Time-of-Flight sensor and the team I was interning with was selected to present  <br/>\r\n       their idea at the global ADI GTC that happened in March 2023.</p>\r\n      <p>\r\n        I worked on making a simulation for my team using Robot Operating System and Gazebo Simulator.<br/>\r\n        The simulation was of a 4-wheel skid-steer drive that streamed sensor data in a C++ script using<br/>\r\n        ROStopics. I got to learn about modelling the physics of a physically realizable system while making this<br/>\r\n        simulation. The package that I created was added as a meta-package to the main codebase. \r\n      </p>\r\n\r\n      <p>ADI planned to port their current sensor into the Robot Operating System-2 environment, for which<br/>\r\n       they needed to check for vulnerabilities. I found this to be a very good way to dip my feet into ROS-2.<br/>\r\n       I wrote a script in C++ using OpenCV to transfer messages from one ROS-2 system instance to another, the <br/>\r\n       existing system faced a lot of latency while receiving images and I helped them improve it but reducing the<br/> \r\n       latency to about 67%. Working on something that was useful to the company felt empowering and motivating to me.\r\n      </p>\r\n\r\n    <h2>My time as an Undergraduate Researcher</h2>\r\n    <p>I was an undergraduate research intern at Ontario Tech University under the guidance of Dr. Scott Nokleby<br/>\r\n       This experience was a research fellowship award to me by MITACS. The prompt of the project was to work on<br/>\r\n       an autonomous wheelchair for children with disabilities like cerebral palsy. This was an experience that taught<br/>\r\n       me a lot about the importance of software architecture for autonomous robots and how to conduct research. <br/>\r\n      </p>\r\n      <p>\r\n        I worked on the D435i depth-camera by Intel for localization and to detect obstacles. One of the requirement<br/>\r\n        was that slopes lower than a 15 degree incline should be traversable and not be treated like an obstalce. To<br/>\r\n        solve this I first ventured into computer vision, I developed a methodology and implemented a python script <br/>calculated the slope of\r\n        obstalces in front of the camera. Although, like many things I didn't get an accuracy of <br/>slope estimation that\r\n        would let me call the task complete. So, after researching I narrowed down the likely cause<br/> to noise from the \r\n        camera and implemented a moving-average filter to help improve the accuracy to 98%.\r\n      </p>\r\n      <p>\r\n        The main part of the project was designing the software architecture and implementation to make autonomous <br/>\r\n        navigation successful. We decided to implement an algorithm called RRT* (Optimized Rapidly Exploring Random Trees)<br/>\r\n        to perform navigation within the map. To avoid collision from unknown obstacles that aren't in the map but can exist<br/>\r\n        (Eg. Trolleys or Humans walking around), we implemented a local planner using a Proportional-Derivative controller<br/>\r\n        modelling the error function in such a way that it produces a controller effort that is inversely-porportional to<br/>\r\n        the distance from the detected obstacle. \r\n      </p>\r\n      <p>\r\n        We also used a Turtlebot-3 Burger robot based on Raspberry Pi 4 and OpenCR to test the entire setup on hardware<br/> and were able to see that the algorithms\r\n        worked successfully.\r\n      </p>\r\n      <h2>My time working at a UAV startup</h2>\r\n      <p>In my 2nd year of college I got the opportunity to work at AEREO (Formerly Aarav Unmanned Systems). I got to work<br/>\r\n      path-planning for Unammned Aerial Vehicles and autonomating testing procedures for them. I got exposure to open-source<br/>\r\n      Frameworks and developing on top of an existing codebase. I got familiar with the Ardupilot flight-stack and worked on <br/>\r\n      creating a Software-In-Loop testbench that was later integrated to a Ground Control Software using TCP and UDP. <br/>\r\n      </p>\r\n\r\n    \r\n   \r\n    \r\n   \r\n\r\n    \r\n    </div>\r\n  </div>\r\n      );\r\n}\r\n\r\nexport default About;","C:\\Users\\aksha\\OneDrive\\Desktop\\portfolio\\src\\Component\\Projects.js",["116","117","118"],[],"import React from 'react';\r\nimport { Link, NavLink } from 'react-router-dom';\r\nimport Navbar from './Navbar';\r\n\r\nfunction Projects() {\r\n    return (\r\n        <div className=\"projects-container\">\r\n        <div className=\"Center\">\r\n        <div className=\"header-container\">\r\n        <div className=\"top-bar\">\r\n        </div>\r\n        <br/>\r\n    <h1>Projects</h1><br/>\r\n    <div>\r\n    <h2>This Website</h2>\r\n      <p>I always wanted to have my own website to have the ability to showcase<br/>\r\n      the things I was doing. I decided to take this up as a personal project.<br/>\r\n      With multiple failed compilations and a lot of debugging sessions, I finally <br/>\r\n      came up with something I feel good about. I'm open to any suggestions! <br/>\r\n      \r\n      </p>\r\n    </div>\r\n    <div>\r\n    <h2>Chatbot with embeddings: Ongoing</h2>\r\n      <p>I'm currently working on making a chatbot using embeddings and vector search<br/>\r\n      for making ChatGPT effective for specialized research topics.\r\n      </p>\r\n    </div>\r\n    <div>\r\n    <h2>Kalman Filters for Reinforcement Learning: Ongoing</h2>\r\n      \r\n      <p>Working towards using Kalman Filters to optimize Bellman updates for Reinforcement<br/>\r\n      learning algorithms to optimize Neural Architecture Search.\r\n      </p>\r\n    </div>\r\n    <hr/>\r\n    <div>\r\n    <h2>Seraphim</h2>\r\n      <div className=\"image-container\">\r\n        <img className=\"project-image\" src=\"/Images/seraphim.png\" alt=\"\" />\r\n      </div>\r\n      <p>Seraphim is a project I was a part of during my time at Project MANAS. Seraphim is an <br/>\r\n      autonomous hexacopter based on the PX4 flight stack and is capable of autonomous waypoint<br/> \r\n      navigation and object detection through a camera module. Seraphim maintains a thrust to <br/>\r\n      weight ratio of 2:1 while take-off. Seraphim can also carry a payload of upto 7 kg.<br/>\r\n      </p>\r\n    </div>\r\n    <hr/>\r\n    <div>\r\n    <h2>KOBU</h2>\r\n    <div className=\"image-container\">\r\n        <img className=\"project-image\" src=\"/Images/kobu.png\" alt=\"\" />\r\n      </div>\r\n      <p>KOBU is an a system of 3 differential-drive robots that aims to achieve multi-robot<br/>\r\n      coverage planning using the concept of space-filling curves like Peano and Hilbert's <br/>\r\n      curves. This was implemented using ROS and Gazebo using C++.\r\n      </p>\r\n    </div>\r\n    <hr/>\r\n    <div>\r\n    <h2>Drone Simulation</h2>\r\n    <div className=\"image-container\">\r\n        <img className=\"project-image\" src=\"/Images/dpo.png\" alt=\"\" />\r\n      </div>\r\n      <p>I made a simulation to visualize the flight of a quadcopter in Gazebo using ROS. This<br/>\r\n       was a Software-In-Loop testbench I made to send coordinates through a vector in C++<br/>\r\n       to the PX4 flight stack.\r\n      </p>\r\n    </div>\r\n    <hr/>\r\n    <div>\r\n    <h2>Slope Detection</h2>\r\n    <div className=\"image-container\">\r\n        <img className=\"project-image\" src=\"/Images/sloped.png\" alt=\"\" />\r\n      </div>\r\n      <p>I worked on implementing slope detection of incident objects through a depth-camera sensor<br/>\r\n       in python and OpenCV. The script was coverted into publishing the slopes calculated through <br/>ROStopics.\r\n      </p>\r\n    </div>\r\n    <hr/>\r\n    <div>\r\n    <h2>Iterative Inverse Kinematics</h2>\r\n    <div className=\"image-container\">\r\n        <img className=\"project-image\" src=\"/Images/ik.png\" alt=\"\" />\r\n      </div>\r\n      <p>This was an implementation of iterative inverse kinematics techniques using Jacobian matrices.<br/>\r\n       I used Newton Raphson root solving method to achieve convergence of a 3 DoF RRR robotic arm. <br/>\r\n      </p>\r\n    </div>\r\n    <div>\r\n    <h2>SHELFY</h2>\r\n    <div className=\"image-container\">\r\n        <img className=\"project-image\" src=\"/Images/shelfy.jpg\" alt=\"\" />\r\n      </div>\r\n      <p>My team took part in the Swadeshi Microprocessor Challenge, hosted by the Govt. of India,.<br/>\r\n      Ministry of Electronics and Information Technology. We were the national semi-finalists and <br/>\r\n      amoung the top 100 teams out of 3000+ participants. The team was given an indiginous FPGA board<br/>\r\n      to develop a product. We chose to build a smart automated storage and retrieval system for <br/>\r\n      warehouse automation. \r\n      </p>\r\n    </div>\r\n    \r\n    \r\n    \r\n    \r\n</div>\r\n    </div>\r\n    </div>\r\n\r\n      );\r\n}\r\n\r\nexport default Projects;","C:\\Users\\aksha\\OneDrive\\Desktop\\portfolio\\src\\Component\\extras.js",["119","120"],[],"import React from 'react';\r\nimport { NavLink } from \"react-router-dom\";\r\nimport { Link } from \"react-router-dom\";\r\n\r\nfunction extras(){\r\n    return (\r\n        <div className = 'navbar'>\r\n            <div>\r\n                <ul classname='navbar-menu'>\r\n                    {/* <li><Link to =\"/\">Home</Link></li> */}\r\n                    <li><Link to =\"/About\">About Me</Link></li>\r\n                </ul>\r\n            </div>\r\n        </div>\r\n    )\r\n}","C:\\Users\\aksha\\OneDrive\\Desktop\\portfolio\\src\\Component\\Navbar.js",[],[],"C:\\Users\\aksha\\OneDrive\\Desktop\\akshatowl.github.io\\src\\index.js",["121","122","123","124","125","126"],[],"import React from 'react';\r\nimport ReactDOM from 'react-dom/client';\r\nimport App from './App';\r\nimport About from './Component/About';\r\nimport Contact from './Component/Contact';\r\nimport Navbar from './Component/Navbar';\r\nimport Projects from './Component/Projects';\r\nimport './App.css'\r\n\r\nimport { Route, Routes, BrowserRouter } from \"react-router-dom\";\r\n\r\nconst root = ReactDOM.createRoot(document.getElementById('root'));\r\nroot.render(\r\n  <React.StrictMode>\r\n    <BrowserRouter>\r\n      <App/>\r\n    </BrowserRouter>\r\n  </React.StrictMode>,\r\n);\r\n","C:\\Users\\aksha\\OneDrive\\Desktop\\akshatowl.github.io\\src\\App.js",[],[],"C:\\Users\\aksha\\OneDrive\\Desktop\\akshatowl.github.io\\src\\Component\\Navbar.js",["127","128","129"],[],"import {React, useRef, useState} from 'react';\r\nimport { NavLink } from 'react-router-dom';\r\n \r\n\r\nfunction Navbar() {\r\n  const navRef = useRef();\r\n  const [isResponsive, setResponsive] = useState(false);\r\n\r\n  const navbarStyle = {\r\n    position: 'fixed',\r\n    top: 0,\r\n    left: 0,\r\n    width: '100%',\r\n    backgroundColor: 'white', // Change this to your desired background color\r\n    zIndex: 999, // Use a high z-index to ensure it's above other elements\r\n    // Add other styling as needed\r\n  };\r\n  const showNavbar = () => {\r\n    setResponsive(!isResponsive);\r\n  }\r\n  return (\r\n    <div className=\"Center\">\r\n      <nav className=\"nav-bar\">\r\n        <ul>\r\n          <li style={{ marginRight: '40px' }}>\r\n            <NavLink to=\"/\" exact activeClassName=\"active-link\" className=\"hover-outline-link\">\r\n              âŒ‚\r\n            </NavLink>\r\n          </li>\r\n          <li style={{ marginRight: '40px' }}>\r\n            <NavLink to=\"/aboutme\" activeClassName=\"active-link\" className=\"hover-outline-link\">\r\n              Experience\r\n            </NavLink>\r\n          </li>\r\n          <li style={{ marginRight: '40px' }}>\r\n            <NavLink to=\"/projects\" activeClassName=\"active-link\" className=\"hover-outline-link\">\r\n              Projects\r\n            </NavLink>\r\n          </li>\r\n          <li style={{ marginRight: '40px' }}>\r\n            <NavLink to=\"/contact\" activeClassName=\"active-link\" className=\"hover-outline-link\">\r\n              Resume\r\n            </NavLink>\r\n          </li>\r\n        </ul>\r\n      </nav>\r\n\r\n      </div>\r\n  );\r\n}\r\n\r\nexport default Navbar;\r\n","C:\\Users\\aksha\\OneDrive\\Desktop\\akshatowl.github.io\\src\\Component\\About.js",["130","131","132"],[],"import React from 'react';\r\nimport { Link, NavLink } from 'react-router-dom';\r\nimport Navbar from './Navbar';\r\n\r\nfunction About() {\r\n    return (\r\n      <div className=\"Center\">\r\n      <div className=\"header-container\">\r\n      <div className=\"top-bar\">\r\n      </div>\r\n      <br/>\r\n    \r\n    <h1>Experience</h1>\r\n    <p>\r\n    <p> I've researched and worked with robots a lot, I managed to gain some useful skills while trying to work<br/>\r\n       on autonomous systems throughout my undergraduate degree. My primary area of work has been towards <br/>Control Systems and heuristic path-planning\r\n       of Unmanned Aerial Vehicles and Unmanned Ground Vehicles.<br/></p>\r\n    <div>\r\n      <p>\r\n      <ul>\r\n        <li>Software Engineering Intern, <a href=\"https://www.analog.com/en/index.html\" style={{ fontSize: '20px', display: 'inline', textDecoration: 'underline' }} target=\"blank\">Analog Devices</a>, India <i>Jan 2023 - May 2023</i></li>\r\n        <li>Undergraduate Research Intern <a href=\"https://www.mitacs.ca/en\" style={{ fontSize: '20px',display: 'inline', textDecoration: 'underline' }} target=\"blank\">(MITACS)</a>, MARS Lab, Ontario Tech University <i>May 2022 - Jul 2022</i></li>\r\n        <li>Head of Automation, <a href=\"https://www.projectmanas.in/\" style={{ fontSize: '20px',display: 'inline', textDecoration: 'underline' }} target=\"blank\">Project MANAS</a> <i>April 2021 - Jul 2022</i></li>\r\n        <li>Embedded Software Intern, <a href=\"https://www.mitacs.ca/en\" style={{ fontSize: '20px',display: 'inline', textDecoration: 'underline' }} target=\"blank\">AEREO</a> , India <i>Oct 2022 - Dec 2022</i></li>\r\n        <li>Team Member,  <a href=\"https://aereo.io/\" style={{ fontSize: '20px',display: 'inline', textDecoration: 'underline' }} target=\"blank\">MBZIRC</a>  <i>Oct 2022 - Dec 2022</i></li>\r\n      </ul>\r\n      </p>\r\n    </div>\r\n    </p>\r\n    \r\n    <h1>My Takeaways</h1>\r\n       <h2>My time at Analog Devices</h2>\r\n      <p>Before arriving for my Master's, I was a Software Engineering Intern at Analog Devices, India or ADI.<br/> \r\n       ADI was working on a Time-of-Flight sensor and the team I was interning with was selected to present  <br/>\r\n       their idea at the global ADI GTC that happened in March 2023.</p>\r\n      <p>\r\n        I worked on making a simulation for my team using Robot Operating System and Gazebo Simulator.<br/>\r\n        The simulation was of a 4-wheel skid-steer drive that streamed sensor data in a C++ script using<br/>\r\n        ROStopics. I got to learn about modelling the physics of a physically realizable system while making this<br/>\r\n        simulation. The package that I created was added as a meta-package to the main codebase. \r\n      </p>\r\n\r\n      <p>ADI planned to port their current sensor into the Robot Operating System-2 environment, for which<br/>\r\n       they needed to check for vulnerabilities. I found this to be a very good way to dip my feet into ROS-2.<br/>\r\n       I wrote a script in C++ using OpenCV to transfer messages from one ROS-2 system instance to another, the <br/>\r\n       existing system faced a lot of latency while receiving images and I helped them improve it but reducing the<br/> \r\n       latency to about 67%. Working on something that was useful to the company felt empowering and motivating to me.\r\n      </p>\r\n\r\n    <h2>My time as an Undergraduate Researcher</h2>\r\n    <p>I was an undergraduate research intern at Ontario Tech University under the guidance of Dr. Scott Nokleby<br/>\r\n       This experience was a research fellowship award to me by MITACS. The prompt of the project was to work on<br/>\r\n       an autonomous wheelchair for children with disabilities like cerebral palsy. This was an experience that taught<br/>\r\n       me a lot about the importance of software architecture for autonomous robots and how to conduct research. <br/>\r\n      </p>\r\n      <p>\r\n        I worked on the D435i depth-camera by Intel for localization and to detect obstacles. One of the requirement<br/>\r\n        was that slopes lower than a 15 degree incline should be traversable and not be treated like an obstalce. To<br/>\r\n        solve this I first ventured into computer vision, I developed a methodology and implemented a python script <br/>calculated the slope of\r\n        obstalces in front of the camera. Although, like many things I didn't get an accuracy of <br/>slope estimation that\r\n        would let me call the task complete. So, after researching I narrowed down the likely cause<br/> to noise from the \r\n        camera and implemented a moving-average filter to help improve the accuracy to 98%.\r\n      </p>\r\n      <p>\r\n        The main part of the project was designing the software architecture and implementation to make autonomous <br/>\r\n        navigation successful. We decided to implement an algorithm called RRT* (Optimized Rapidly Exploring Random Trees)<br/>\r\n        to perform navigation within the map. To avoid collision from unknown obstacles that aren't in the map but can exist<br/>\r\n        (Eg. Trolleys or Humans walking around), we implemented a local planner using a Proportional-Derivative controller<br/>\r\n        modelling the error function in such a way that it produces a controller effort that is inversely-porportional to<br/>\r\n        the distance from the detected obstacle. \r\n      </p>\r\n      <p>\r\n        We also used a Turtlebot-3 Burger robot based on Raspberry Pi 4 and OpenCR to test the entire setup on hardware<br/> and were able to see that the algorithms\r\n        worked successfully.\r\n      </p>\r\n      <h2>My time working at a UAV startup</h2>\r\n      <p>In my 2nd year of college I got the opportunity to work at AEREO (Formerly Aarav Unmanned Systems). I got to work<br/>\r\n      path-planning for Unammned Aerial Vehicles and autonomating testing procedures for them. I got exposure to open-source<br/>\r\n      Frameworks and developing on top of an existing codebase. I got familiar with the Ardupilot flight-stack and worked on <br/>\r\n      creating a Software-In-Loop testbench that was later integrated to a Ground Control Software using TCP and UDP. <br/>\r\n      </p>\r\n      <h2>My time working in Project MANAS</h2>\r\n      <p>During my undergraduate degree I was part of Project MANAS, the robotics student project of MIT Manipal. During <br/>\r\n      2021-2022 I led the automation front of the team for AUVSI SUAS 2022 and a driverless car for the Mahindra Rize Prize<br/>\r\n      Challenge. I got to work hands on with a lot of sensor modules and communication protocols like I2C, UART, SPI, CAN   <br/>\r\n      for autonomous robots. I worked quite a bit on the lateral controller of a driverless car and helped implement a <br/>\r\n      modified Stanley Controller with feedforward for lane centering.\r\n      </p>\r\n      <h2>My time working for MBZIRC</h2>\r\n      <p>I was part of Team Luna, representing India among the top 13 teams internationally as semi-finalists for the Mohamed <br/>\r\n       Bin Zayed International Robotics Challenge (MBZIRC). I worked on Forward and Inverse Kinematics of 6-axis <br/>\r\n      robotic arm on top of a surface vehicle as part of the competition prompt. <br/>\r\n      \r\n      </p>\r\n\r\n    </div>\r\n  </div>\r\n      );\r\n}\r\n\r\nexport default About;","C:\\Users\\aksha\\OneDrive\\Desktop\\akshatowl.github.io\\src\\Component\\Contact.js",["133","134","135","136"],[],"import React from 'react';\r\nimport { Document, Page, Text } from 'react-pdf';\r\n\r\nfunction Contact() {\r\n  const pdfFile = '/Akshat_Pandey.pdf'; // Replace with the actual path to your PDF file\r\n\r\n  return (\r\n    <div>\r\n    <h1>Resume</h1>\r\n    <iframe\r\n      title=\"Resume\"\r\n      src=\"/Akshat_Pandey.pdf\" // Update the path to your PDF file\r\n      width=\"100%\"\r\n      height=\"1200px\"\r\n    />\r\n  </div>\r\n  );\r\n}\r\n\r\nexport default Contact;\r\n","C:\\Users\\aksha\\OneDrive\\Desktop\\akshatowl.github.io\\src\\Component\\Projects.js",["137","138","139"],[